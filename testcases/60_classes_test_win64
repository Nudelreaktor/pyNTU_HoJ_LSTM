python lstm.py -c 61 -s 172 -ls 200,200 -dop 'pickles/full_sets/hoj_sphere_parts_full.bin_data' -e 50 -tl 'P001,P002,P004,P005,P008,P009,P013,P014,P015,P016,P017,P018,P019,P025,P027,P028,P031,P034,P035,P038' -b 32 -a 'relu' -ra 'hard_sigmoid' -p 'full_sets/cs_hoj_sphere_parts'

python lstm.py -c 61 -s 27 -ls 200,200 -dop 'pickles/full_sets/3d_points_global_full.bin_data' -e 50 -tl 'C002,C003' -b 32 -a 'relu' -ra 'hard_sigmoid' -p 'full_sets/cc_3D_global'
python lstm.py -c 61 -s 27 -ls 200,200 -dop 'pickles/full_sets/3d_points_local_full.bin_data' -e 50 -tl 'C002,C003' -b 32 -a 'relu' -ra 'hard_sigmoid' -p 'full_sets/cc_3D_local'
python lstm.py -c 61 -s 84 -ls 200,200 -dop 'pickles/full_sets/hoj_full.bin_data' -e 50 -tl 'C002,C003' -b 32 -a 'relu' -ra 'hard_sigmoid' -p 'full_sets/cc_hoj'
python lstm.py -c 61 -s 88 -ls 200,200 -dop 'pickles/full_sets/hoj_parts_full.bin_data' -e 50 -tl 'C002,C003' -b 32 -a 'relu' -ra 'hard_sigmoid' -p 'full_sets/cc_hoj_parts'
python lstm.py -c 61 -s 168 -ls 200,200 -dop 'pickles/full_sets/hoj_cylinder_full.bin_data' -e 50 -tl 'C002,C003' -b 32 -a 'relu' -ra 'hard_sigmoid' -p 'full_sets/cc_hoj_cylinder'
