python lstm.py -c 61 -s 168 -ls 200,200 -dop 'pickles/full_sets/hoj_cylinder_full.bin_data' -e 50 -tl 'P001,P002,P004,P005,P008,P009,P013,P014,P015,P016,P017,P018,P019,P025,P027,P028,P031,P034,P035,P038' -b 32 -a 'relu' -ra 'hard_sigmoid' -p 'full_sets/cs_hoj_cylinder'
